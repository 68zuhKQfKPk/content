[MODEL: dataset=mysql_raw, model=Audit, content_id=***, version=***]
| alter XDM_audit_Main_operation_type = json_extract_scalar(_raw_log, ""$.class"")
| filter XDM_Audit_Main_operation_type = ""audit""
| alter XDM_Database_time = json_extract_scalar(_raw_log, ""$.timestamp""),
XDM_Audit_Main_operation_sub_type =  json_extract_scalar(_raw_log, ""$.event""),
XDM_Audit_audited_resource_id =  json_extract_scalar(_raw_log, ""$.startup_data.server_id""),
XDM_Audit_audited_resource_type =  json_extract_scalar(_raw_log, ""$.startup_data.os_version""),
XDM_Audit_audited_resource_sub_type =  json_extract_scalar(_raw_log, ""$.startup_data.mysql_version""),
XDM_Audit_audited_resource_id =  json_extract_scalar(_raw_log, ""$.shutdown_data.server_id"");

[MODEL: dataset=mysql_raw, model=Auth, content_id=***, version=***]
| alter XDM_auth_Main_operation_type = json_extract_scalar(_raw_log, ""$.class"")
| filter XDM_auth_Main_operation_type = ""connection""
| alter XDM_auth_time = json_extract_scalar(_raw_log, ""$.timestamp""),
