XDM_Auth_Target_os = json_extract_scalar(_raw_log, ""$.login.os""),
XDM_Auth_Target_ipv4 = json_extract_scalar(_raw_log, ""$.login.ip"");

[MODEL: dataset=mysql_raw, model=Database, content_id=***, version=***]
| alter XDM_database_Main_operation_type = json_extract_scalar(_raw_log, ""$.class"")
